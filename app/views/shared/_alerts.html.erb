<% if flash[:alert] %>
<%= js add_gritter(flash[:alert], title: "Ang Portfolio", sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Ang Portfolio", sticky: false) %>
<% else %>
 <%= js add_gritter(flash[:notice], title: "Ang Portfolio", sticky: false) %>
<% end %>
