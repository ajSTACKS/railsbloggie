<h1>Porfolio nicca</h1>


<%= link_to "Create New item", new_portfolio_url %>
<% @portfolio_items.each do |portfolio_item| %>
  <p><%= portfolio_item.title %></p>
  <p><%= portfolio_item.subtitle %></p>
  <p><%= portfolio_item.body %></p>
  <p><%= image_tag portfolio_item.thumb_img  unless portfolio_item.thumb_img.nil? %></p>
  <%= link_to "Edit", edit_portfolio_path(portfolio_item.id) %>

<% end %>
